{
  "name": "simple-db",
  "version": "1.0.0",
  "description": "Its a simple document based database project created on top of rocksdb",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "node --experimental-strip-types src/index.ts",
    "lint": "eslint . --ext .ts"
